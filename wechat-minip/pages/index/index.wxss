.m-page {
  width: 100vw;
  height: 100vh;
  padding-bottom: 0;
}

.material .m-navbar-title {
  padding-left: var(--wingBlank);
}

.m-navbar,
.m-list {
  margin-bottom: 32px;
}

.m-navbar.m-onswiper {
  --percent: calc(var(--swiperX, 0) / var(--screenWidth));
  transition: var(--swiperX, all) ease-in-out var(--dur2);
}

.m-navbar.m-onswiper[data-pos='0'] {
  opacity: 0;
}

.prev .m-navbar.m-onswiper[data-pos='0'] {
  opacity: calc(0 - var(--percent));
}

.prev .m-navbar.m-onswiper[data-pos='-1'] {
  opacity: calc(1 + var(--percent));
}

.next .m-navbar.m-onswiper[data-pos='0'] {
  opacity: var(--percent);
}

.next .m-navbar.m-onswiper[data-pos='1'] {
  opacity: calc(1 - var(--percent));
}

.p-tabbar {
  --tabbarHeight: 54px;
  position: fixed;
  z-index: 9;
  left: 0;
  right: 0;
  bottom: 0;
  border-width: 1px 0 0 0;
  background-color: var(--navbarColor);
}

.p-tabbar .m-tab {
  flex-direction: column;
  padding-top: 1px;
  font-size: var(--fs0);
}

.p-tabbar .m-tab[data-pos='0'] {
  font-weight: 400;
}

.p-tabbar .m-tab:after {
  display: none;
}

.p-tabbar .m-tab [data-icon]:before {
  font-size: 28px;
  font-weight: 500;
}

swiper {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  height: 100vh;
}

.p-list-title {
  padding: 4px var(--wingBlank);
}
