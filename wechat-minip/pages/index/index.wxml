<!--index/index.wxml-->
<view
  change:data-disable-on-swiper="{{moui.removeEffectOfOnSwiperTrans}}"
  class="m-page {{_.ui.wrapcls}}"
  data-disable-on-swiper="{{disableOnSwiper}}"
  data-emit-effect-removed="afterRemoveSwiperEffect"
  style="{{_.ui.cssvar}}">
  <wxs src="../../styles/moui/index.wxs" module="moui" />
  <include src="navigator.wxml" />
  <swiper
    bindanimationfinish="{{moui.onSwiperTrans}}"
    bindtransition="{{moui.onSwiperTrans}}"
    current="{{swiperIndex}}"
    data-emit-swiperend="onChangeTab"
    duration="320">
    <swiper-item wx:for="01" wx:key="*this">
      <scroll-view enable-flex="1" scroll-y="1">
        <view class="m-navbar" />
        <include src="components.wxml" wx:if="{{index===0}}" />
        <include src="settings.wxml" wx:elif="{{index===1}}" />
      </scroll-view>
    </swiper-item>
  </swiper>
</view>
